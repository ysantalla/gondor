{
  "name": "server",
  "version": "1.0.0",
  "description": "Nestjs w/ Apollo server, Prisma like ORM-layer",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -- node dist/main.js",
    "dev": "dotenv -e .env.dev -- nodemon -e ts,graphql -x ts-node src/main.ts",
    "debug": "dotenv -e .env.dev -- nodemon -e ts,graphql -x ts-node --inspect src/main.ts",
    "build": "npm run clean && graphql codegen && tsc",
    "clean": "rm -rf ./dist",
    "prisma:up": "docker-compose -f prisma/docker-compose.yml up",
    "prisma": "prisma"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.2",
    "@nestjs/core": "^5.3.2",
    "@nestjs/graphql": "^5.1.2",
    "apollo-server-cache-memcached": "^0.1.3",
    "apollo-server-express": "^2.0.5",
    "bcryptjs": "^2.4.3",
    "graphql": "^0.13.2",
    "graphql-binding": "^2.2.4",
    "graphql-import": "^0.6.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^3.1.1",
    "graphql-type-json": "^0.2.1",
    "jsonwebtoken": "^8.3.0",
    "mkdirp": "^0.5.1",
    "prisma-binding": "^2.1.5",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.1",
    "shortid": "^2.2.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.3.1",
    "@types/bcryptjs": "^2.4.1",
    "@types/graphql": "^14.0.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.9.4",
    "@types/shortid": "^0.0.29",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "^2.16.7",
    "nodemon": "^1.18.4",
    "prisma": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
